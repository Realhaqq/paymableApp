<?xml version="1.0" encoding="utf-8"?>

<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f6f6f6"

    android:fitsSystemWindows="true">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <include layout="@layout/toolbar2"/>
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="330dp"
            app:cardCornerRadius="2dp"
            android:elevation="5dp"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="16.7dp"
            android:layout_marginLeft="16.7dp"
            android:layout_marginTop="13dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <customfonts.MyTextView_Roboto_Regular
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:fontFamily="sans-serif"
                    android:textStyle="normal"
                    android:paddingTop="16.7dp"
                    android:paddingLeft="23.3dp"
                    android:textColor="@color/black_txt_Color"
                    android:text="Transfer Money"/>




                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/signin_rect"
                    android:elevation="1dp">


                    <Spinner
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/sp_bank"
                        />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/signin_rect"
                    android:elevation="1dp">

                    <customfonts.EditText_Roboto_Regular
                        android:id="@+id/account_no"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/transparent"
                        android:fontFamily="sans-serif"
                        android:hint="Account Number"
                        android:paddingBottom="15.7dp"
                        android:paddingLeft="10dp"
                        android:paddingTop="15.7dp"
                        android:textColor="@color/black_txt_Color"
                        android:textColorHint="@color/black_txt_Color"
                        android:textSize="15sp"
                        android:inputType="number"
                        android:singleLine="true"
                        android:textStyle="normal" />
                </LinearLayout>

                <View
                    android:layout_width="133.7dp"
                    android:layout_height="10dp"
                    android:layout_marginLeft="21.3dp"
                    android:layout_marginTop="3dp"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/signin_rect"
                    android:elevation="1dp">

                    <customfonts.EditText_Roboto_Regular
                        android:id="@+id/amount"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/transparent"
                        android:fontFamily="sans-serif"
                        android:hint="Amount"
                        android:paddingBottom="15.7dp"

                        android:paddingLeft="10dp"
                        android:paddingTop="15.7dp"
                        android:textColor="@color/black_txt_Color"
                        android:textColorHint="@color/black_txt_Color"
                        android:textSize="15sp"
                        android:inputType="number"
                        android:singleLine="true"
                        android:textStyle="normal" />
                </LinearLayout>


                <View
                    android:layout_width="133.7dp"
                    android:layout_height="1dp"
                    android:layout_marginLeft="21.3dp"
                    android:layout_marginTop="3dp"
                    android:background="#ececec"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:layout_marginTop="25.3dp">


                    <customfonts.Button_Roboto_Medium
                        android:id="@+id/btn_continue"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:elevation="1dp"
                        android:background="@drawable/signin_rect"
                        android:gravity="center_horizontal"
                        android:fontFamily="sans-serif"
                        android:textStyle="normal"
                        android:paddingTop="15.7dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginBottom="10dp"
                        android:textColor="@color/bg_blue"
                        android:text="TRANSFER"/>
                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>

    </LinearLayout>
